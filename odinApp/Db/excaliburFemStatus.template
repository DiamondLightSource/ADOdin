# Overall alarm record for the per FEM housekeeping
record(calc, "$(P)$(R)$(ADDRESS):DEVSTA10")
{
    field(INPA, "$(P)$(R)DEVSTA11 MS CP")
    field(INPB, "$(P)$(R)EFuseIdMatches MS CP")
    field(INPC, "$(P)$(R)FemInitOk MS CP")
    field(ACKT, "NO")
}
