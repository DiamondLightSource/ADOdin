    field(DTYP, "asynInt32")
    field(VAL,  "0")
    field(OUT,  "@asyn($(PORT),0,$(TIMEOUT))_ODI_config/lv_enable")
    field(ZNAM, "Off")
    field(ONAM, "On")
}

#% archiver 1 monitor
record(bi, "$(P)$(R)CONFIG:ACQUIRE:LvControl_RBV")
{
    field(DTYP, "asynInt32")
    field(SCAN,  "I/O Intr")
    field(INP,  "@asyn($(PORT),0,$(TIMEOUT))_ODI_status/lv_enabled")
    field(ZNAM, "Off")
    field(ONAM, "On")
}

