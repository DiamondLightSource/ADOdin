[
  {
    "fr_setup": {
      "fr_ready_cnxn": "tcp://127.0.0.1:$RD_PORT",
      "fr_release_cnxn": "tcp://127.0.0.1:$RL_PORT"
    }
  },
  {
    "plugin": {
      "load": {
        "index": "hdf",
        "name": "FileWriterPlugin",
        "library": "$OD_ROOT/prefix/lib/libHdf5Plugin.so"
      }
    }
  },
  {
    "plugin": {
      "load": {
        "index": "view",
        "name": "LiveViewPlugin",
        "library": "$OD_ROOT/prefix/lib/libLiveViewPlugin.so"
      }
    }
  },
  {
    "plugin": {
      "load": {
        "index": "excalibur",
        "name": "ExcaliburProcessPlugin",
        "library": "$DETECTOR_ROOT/prefix/lib/libExcaliburProcessPlugin.so"
      }
    }
  },
  {
    "plugin": {
      "connect": {
        "index": "excalibur",
        "connection": "frame_receiver"
      }
    }
  },
  {
    "plugin": {
      "connect": {
        "index": "hdf",
        "connection": "excalibur"
      }
    }
  },
  {
    "plugin": {
      "connect": {
        "index": "view",
        "connection": "excalibur"
      }
    }
  },
  {
    "hdf": {
      "dataset": "data"
    }
  },
  {
    "view": {
      "dataset_name": "data",
      "live_view_socket_addr": "tcp://0.0.0.0:$LIVE_VIEW_PORT"
    }
  }
]
