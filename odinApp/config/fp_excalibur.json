[
  {
    "fr_setup": {
      "fr_ready_cnxn": "tcp://$IP:$RD_PORT",
      "fr_release_cnxn": "tcp://$IP:$RL_PORT"
    }
  },
  {
    "plugin": {
      "load": {
        "index": "hdf",
        "name": "FileWriterPlugin",
        "library": "$FW_ROOT/prefix/lib/libHdf5Plugin.so"
      }
    }
  },
  {
    "plugin": {
      "load": {
        "index": "excalibur",
        "name": "ExcaliburProcessPlugin",
        "library": "$PP_ROOT/prefix/lib/libExcaliburProcessPlugin.so"
      }
    }
  },
  {
    "plugin": {
      "connect": {
        "index": "excalibur",
        "connection": "frame_receiver"
      }
    }
  },
  {
    "plugin": {
      "connect": {
        "index": "hdf",
        "connection": "excalibur"
      }
    }
  },
  {
    "hdf": {
      "dataset": "data"
    }
  }
]
