[
  {
    "fr_setup": {
      "fr_ready_cnxn": "tcp://127.0.0.1:$RD_PORT",
      "fr_release_cnxn": "tcp://127.0.0.1:$RL_PORT"
    },
    "meta_endpoint": "tcp://*:$META_PORT"
  },
  {
    "plugin": {
      "load": {
        "index": "hdf",
        "name": "FileWriterPlugin",
        "library": "$OD_ROOT/prefix/lib/libHdf5Plugin.so"
      }
    }
  },
  {
    "plugin": {
      "load": {
        "index": "eiger",
        "name": "EigerProcessPlugin",
        "library": "$DETECTOR_ROOT/prefix/lib/libEigerProcessPlugin.so"
      }
    }
  },
  {
    "plugin": {
      "connect": {
        "index": "eiger",
        "connection": "frame_receiver"
      }
    }
  },
  {
    "plugin": {
      "connect": {
        "index": "hdf",
        "connection": "eiger"
      }
    }
  },
  {
    "hdf": {
      "dataset": "data"
    }
  }
]
