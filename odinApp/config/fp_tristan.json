[
  {
    "fr_setup": {
      "fr_ready_cnxn": "tcp://127.0.0.1:$RD_PORT",
      "fr_release_cnxn": "tcp://127.0.0.1:$RL_PORT"
    },
    "meta_endpoint": "tcp://*:$META_PORT"
  },
  {
    "plugin": {
      "load": {
        "index": "hdf",
        "name": "FileWriterPlugin",
        "library": "$ODIN_DATA/prefix/lib/libHdf5Plugin.so"
      }
    }
  },
  {
    "plugin": {
      "load": {
        "index": "latrd",
        "name": "LATRDProcessPlugin",
        "library": "$DETECTOR_ROOT/prefix/lib/libLATRDProcessPlugin.so"
      }
    }
  },
  {
    "plugin": {
      "connect": {
        "index": "latrd",
        "connection": "frame_receiver"
      }
    }
  },
  {
    "plugin": {
      "connect": {
        "index": "hdf",
        "connection": "latrd"
      }
    }
  },
  {
    "latrd": {
      "process": {
        "number": $PROC_NUMBER,
        "rank": $PROC_RANK
      },
      "sensor": {
        "width": $WIDTH,
        "height": $HEIGHT
      }
    }
  },
  {
    "hdf": {
      "process": {
        "number": $PROC_NUMBER,
        "rank": $PROC_RANK
      }
    }
  },
  {
    "hdf": {
      "dataset": {
        "raw_data": {
          "datatype": "uint64",
          "chunks": [524288]
        }
      }
    }
  },
  {
    "hdf": {
      "dataset": {
        "event_id": {
          "datatype": "uint32",
          "chunks": [524288]
        }
      }
    }
  },
  {
    "hdf": {
      "dataset": {
        "event_time_offset": {
          "datatype": "uint64",
          "chunks": [524288]
        }
      }
    }
  },
  {
    "hdf": {
      "dataset": {
        "event_energy": {
          "datatype": "uint32",
          "chunks": [524288]
        }
      }
    }
  },
  {
    "hdf": {
      "dataset": {
        "image": {
          "datatype": "uint16",
          "dims": [512, 2048],
          "chunks": [1, 512, 2048]
        }
      }
    }
  },
  {
    "hdf": {
      "dataset": {
        "cue_timestamp_zero": {
          "datatype": "uint64",
          "chunks": [524288]
        }
      }
    }
  },
  {
    "hdf": {
      "dataset": {
        "cue_id": {
          "datatype": "uint16",
          "chunks": [524288]
        }
      }
    }
  },
  {
    "hdf": {
      "dataset": {
        "data": {
          "datatype": "uint16",
          "dims": [1,1],
          "chunks": [1,1,1]
        }
      }
    }
  },
  {
    "hdf": {
      "dataset": {
        "time_slice": {
          "datatype": "uint32",
          "chunks": [40]
        }
      }
    }
  }
]

